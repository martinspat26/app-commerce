        }
    
        a {
            color: #EC9706;
        }
    
        #logo {
            text-align: center;
            padding: 10px 0;
            background: #EC9706;
        }
        
        #logo hr {
            display: block;
            height: 1px;
            overflow: hidden;
            background: #EC9706;
            border: 0;
            padding: 0;
            margin: 30px 0 20px 0;
        }

        #footer {
            text-align: center;
            background: #EC9706;
        }
    
        .claim {
            text-transform: uppercase;
            color: #EC9706;
        }

        button {
            margin-left: 10px;
        }

